{
  "name": "rzl-utils-js",
  "version": "0.1.5",
  "type": "module",
  "description": "A modern, lightweight set of JavaScript utility functions for everyday development, crafted to enhance code readability and maintainability.",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types/types/index.d.ts"
    }
  },
  "scripts": {
    "format": "prettier --check .",
    "lint": "eslint . --ext .ts,.js",
    "bump:patch": "npm version patch",
    "bump:minor": "npm version minor",
    "bump:major": "npm version major",
    "barrel": "npx barrelsby --directory src --single --delete",
    "build": "npm run barrel && tsup && npm run build:types",
    "build:types": "tsc -p tsconfig.build.json",
    "dev": "tsc --watch",
    "prepare": "npm run build",
    "check-publish": "npm pack --dry-run"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rzl-app/rzl-utils-js.git"
  },
  "author": {
    "name": "Rizalvin Dwiky",
    "email": "rizalvindwiky1998@gmail.com"
  },
  "keywords": [
    "javascript",
    "typescript",
    "utils",
    "helpers",
    "library",
    "rzl",
    "rzl-utils-js"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rzl-app/rzl-utils-js/issues"
  },
  "homepage": "https://github.com/rzl-app/rzl-utils-js#readme",
  "devDependencies": {
    "@types/lodash": "^4.17.20",
    "@types/node": "^20.19.4",
    "lodash": "^4.17.21",
    "release-please": "^17.1.1",
    "tsup": "^8.5.0",
    "typescript": "^5.2.0"
  },
  "dependencies": {
    "date-fns": "^4.1.0"
  }
}